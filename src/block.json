{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/first-block",
	"version": "0.1.0",
	"title": "First Block",
	"category": "media",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"content": {
			"type": "string",
			"default": "Default Text here for string type data"
		}
	},
	"supports": {
		"anchor" : true,
		"align": true,
		"color": {
			"gradients": true,
			"background": true,
			"text": true
		},
		"spacing": {
			"margin":true,
			"padding":true,
			"blockGap":true
		},
	    "typography": {			
			"fontSize": true,			
			"lineHeight": true,			
			"textAlign": true
		}
	},
	"textdomain": "first-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
